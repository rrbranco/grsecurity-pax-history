diff -NurpX linux-3.8.13-pax/Documentation/dontdiff linux-3.8.13/scripts/gcc-plugin.sh linux-3.8.13-pax/scripts/gcc-plugin.sh
--- linux-3.8.13/scripts/gcc-plugin.sh	1970-01-01 01:00:00.000000000 +0100
+++ linux-3.8.13-pax/scripts/gcc-plugin.sh	2013-02-19 01:14:44.669772785 +0100
@@ -0,0 +1,17 @@
+#!/bin/bash
+plugincc=`$1 -E -shared - -o /dev/null -I\`$3 -print-file-name=plugin\`/include 2>&1 <<EOF
+#include "gcc-plugin.h"
+#include "tree.h"
+#include "tm.h"
+#include "rtl.h"
+#ifdef ENABLE_BUILD_WITH_CXX
+#warning $2
+#else
+#warning $1
+#endif
+EOF`
+if [ $? -eq 0 ]
+then
+	[[ "$plugincc" =~ "$1" ]] && echo "$1"
+	[[ "$plugincc" =~ "$2" ]] && echo "$2"
+fi
